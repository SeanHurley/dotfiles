[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  oops = commit --amend --no-edit
  staged = diff --staged
  aa = add .
  bd = branch -D
  bc = co -b
  pfl = push --force-with-lease
[push]
	default = current
[core]
	editor = /usr/bin/vim
[url "https://github.com"]
	insteadOf = git://github.com
[init]
	templatedir = ~/.git_template
